/* pages/introduction/introduction.wxss */
.pre {
  background-color: #f6f8fa;
  padding: 5px;
  margin-top: 10px;
  margin-bottom: 10px;
  border-radius: 5px;
  font-size: 13px;
  color: #24292e;
  font-family: monospace;
  white-space: pre;
  overflow: scroll;
}
.code {
  font-style: italic;
  color: #005cc5;
  margin-left: 1px;
  margin-right: 3px;
  font-family: monospace;
}
.td {
  padding: 3px;
  border-right: 1px solid #dfe2e5;
  border-bottom: 1px solid #dfe2e5;
  display: flex;
  justify-content: center;
  align-items: center;
}